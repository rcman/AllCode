initstars:
        FOR i = 1 TO 19
'          PRINT i, x1%, y1%
          x1% = RND * 640
          y1% = RND * 350
          stars(1, 1, i) = x1%
          stars(1, 2, i) = y1%
          NEXT i
          RETURN

drawstars:
        FOR i = 1 TO 19
        'PUT (stars(1, 1, i), start(1, 2, i)), Box%, XOR
        PUT (stars(1, 1, i), stars(1, 2, i)), Box%
        ' PUT (x1%, y1%), Box%
        NEXT i
        RETURN


movestars:
       
        FOR i = 1 TO 19
        
        PUT (stars(1, 1, i), stars(1, 2, i)), Box%, XOR
        'PUT (stars(1, 1, i), stars(1, 2, i)), Box%, XOR
        stars(1, 1, i) = stars(1, 1, i) - 1
        IF stars(1, 1, i) < 1 THEN stars(1, 1, i) = 590
        ' PUT (x1%, y1%), BOX%
        NEXT i
        RETURN



