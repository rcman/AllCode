initstars:
        FOR i = 1 TO 39
'          PRINT i, x1%, y1%
          x1% = RND * 640
          y1% = RND * 350
          stars(1, 1, i) = x1%
          stars(1, 2, i) = y1%
          NEXT i
          RETURN

drawstars:
        FOR j = 1 TO 15
        FOR i = 1 TO 19
        'PUT (stars(1, 1, i), start(1, 2, i)), Box%, XOR
        PUT (stars(1, 1, i), stars(1, 2, i)), Box%(j)
        ' PUT (x1%, y1%), Box%
        NEXT i
        NEXT j

        RETURN


movestars:
        PUT (stars(1, 1, sn), stars(1, 2, sn)), Box%(ss), XOR
        'PUT (stars(1, 1, i), stars(1, 2, i)), Box%, XOR
        'stars(1, 1, i) = stars(1, 1, i) - 1
        IF stars(1, 1, i) < 1 THEN stars(1, 1, i) = 590
        ' PUT (x1%, y1%), BOX%
        ss = ss + 1
        sn = sn + 1
        IF ss > 15 THEN ss = 1
        IF sn = 19 THEN sn = 1
        RETURN



